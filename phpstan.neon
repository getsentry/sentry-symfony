includes:
    - phpstan-baseline.neon

parameters:
    level: 7
    paths:
        - src
        - tests
    excludes_analyse:
        - tests/End2End/App
        - tests/Tracing/Symfony/RequestEventsListenerTest.php
    dynamicConstantNames:
        - Symfony\Component\HttpKernel\Kernel::VERSION
        - Doctrine\DBAL\Version::VERSION
    stubFiles:
        - tests/Stubs/Profile.phpstub
